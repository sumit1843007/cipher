header.header 
    nav.nav.nav--tours
        a.nav__el(href='/')  All tours 
        .header__logo
            img(src='/img/logo-white.png' alt='Natours logo')  
    nav.nav.nav--tours 
        //- - console.log(user) 
        if user
            a.nav__el.nav__el--logout(href='/') Log out
            a.nav__el(href='/account')
                img.nav__user-img(src=`/img/users/${user.photo}`)
        else  
            a.nav__el.nav__el--cta(href='/signUp') Sign up    
            a.nav__el.nav__el--cta(href='/login') Login 
